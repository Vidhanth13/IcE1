<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/txtHelloeWorld"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5C5F6E"
    tools:context=".HelloWorlD">

    <Button
        android:id="@+id/btnSpanish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spanish"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="64dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnFrench"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="French"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="152dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnGerman"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="German"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="244dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnAfrikaans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Afrikaans"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="341dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnZulu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zulu"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="436dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnBackToEnglish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back to English"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="523dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txtHelloeWorld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="644dp"
        tools:ignore="DuplicateIds,MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>